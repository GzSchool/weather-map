/* pages/inputtips/inputtips.wxss */
.map-inputSearch-input{
  height: 80rpx;
  line-height: 80rpx;
  width: 100%;
  box-sizing: border-box;
  font-size: 30rpx;
  padding: 0 10px;
  background-color: #fff;
  position: fixed;
  top: 0px;
  left: 0;
  z-index: 1000;
  border-bottom:1px solid #c3c3c3;
}
.map_container{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 50px;
  bottom: 0;
  left: 0;
  right: 0;
}
.map{
  width: 100%;
  height: 100%;
}
.map-inputSearch-input input{
  border: 1px solid #ddd;
  border-radius: 5px;
  height: 60rpx;
  line-height: 60rpx;
  width: 100%;
  box-sizing: border-box;
  padding: 0 5px;
  margin-top: 10rpx;
}
.map-box{
  margin: 0 0px;
  width: 100%;
  border-bottom:1px solid #c3c3c3;
  height: 80rpx;
  line-height: 80rpx;
}
.map-box:last-child{border: none;}
.map-search-list{
  position: fixed;
  top: 80rpx;
  left: 0;
  width: 100%;
  height: 80%;
  z-index: 1000;
  background-color: #ffffff;
}